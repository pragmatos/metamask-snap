{
  "name": "snap",
  "version": "0.1.0",
  "private": true,
  "description": "The 'Hello, world!' of MetaMask Snaps, now written in TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "webpack && yarn build:snap",
    "build:webpack": "webpack",
    "build:snap": "yarn build:snap:postprocess && mm-snap manifest --fix && yarn build:snap:eval",
    "build:snap:eval": "mm-snap eval -b dist/snap.js",
    "build:snap:manifest": "mm-snap manifest --fix",
    "build:snap:postprocess": "node ./post-process.js",
    "build:mm": "mm-snap build",
    "clean": "rimraf dist build ",
    "compile": "babel src -d lib",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "serve": "mm-snap serve",
    "start": "mm-snap --version && yarn build && yarn serve",
    "test": "jest --forceExit",
    "test:ci": "yarn test --silent"
  },
  "dependencies": {
    "@0xpolygonid/js-sdk": "file:0xpolygonid-js-sdk-1.0.0-beta.3.tgz",
    "@iden3/js-iden3-core": "^1.0.0-beta.2",
    "@iden3/js-jwz": "^1.0.0-beta.2",
    "@iden3/js-merkletree": "^1.0.0-beta.4",
    "@js-temporal/polyfill": "^0.4.3",
    "@metamask/key-tree": "^6.0.0",
    "@metamask/providers": "10.2.1",
    "@metamask/snaps-types": "^0.32.2",
    "@metamask/snaps-ui": "^0.32.2",
    "intl": "^1.2.5",
    "js-base64": "^3.7.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-flow-strip-types": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/register": "^7.18.9",
    "@babel/types": "^7.21.2",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^2.6.0",
    "@metamask/eslint-config": "^10.0.0",
    "@metamask/eslint-config-jest": "^10.0.0",
    "@metamask/eslint-config-nodejs": "^10.0.0",
    "@metamask/eslint-config-typescript": "^10.0.0",
    "@metamask/snaps-cli": "^0.32.2",
    "@metamask/snaps-webpack-plugin": "^0.32.2",
    "@types/jest": "^29.2.2",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.3.1",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
